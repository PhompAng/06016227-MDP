<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phompang.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".autocompletetextview.AutoCompleteTextViewActivity" />
        <activity android:name=".spinner.SpinnerActivity" />
        <activity android:name=".radio.RadioActivity" />
        <activity android:name=".checkbox.CheckBoxActivity" />
        <activity android:name=".listview.ListActivity" />
        <activity android:name=".listview.ListDetailActivity"></activity>
    </application>

</manifest>
